{
  "name": "gle-portfolio",
  "description": "GLE Portfolio Site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-db": "prisma migrate dev --name init",
    "migrate-db": "prisma migrate dev",
    "view-db": "npx prisma studio",
    "generate": "npm run generate-db && npm run generate-server && npm run generate-client",
    "generate-db": "prisma generate",
    "generate-client": "rimraf ./graphql/client/ && graphql-codegen --config codegen-client.ts",
    "generate-server": "rimraf ./graphql/server/ && graphql-codegen --config codegen-server.ts",
    "postinstall": "npm run generate"
  },
  "dependencies": {
    "@mantine/core": "^7.12.2",
