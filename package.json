    "@prisma/client": "5.19.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/styled-components": "^5.1.28",
    "@types/three": "^0.167.1",
    "eslint": "latest",
