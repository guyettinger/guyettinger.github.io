    "@react-three/fiber": "^8.16.8",
    "@tabler/icons-react": "^3.14.0",
    "@tanstack/react-query": "^5.53.1",
    "emoji-picker-react": "^4.11.1",
    "framer-motion": "^11.3.24",
    "github-markdown-css": "^5.6.1",
    "gle-components": "^1.0.3",
    "gle-r3f-components": "^1.0.5",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.7.0",
    "mapbox-gl": "^3.6.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "^4.11.0",
